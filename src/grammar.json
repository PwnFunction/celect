{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "sql",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "_statement"
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "select_statement"
        }
      ]
    },
    "select_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "SELECT"
        },
        {
          "type": "SYMBOL",
          "name": "select_list"
        },
        {
          "type": "STRING",
          "value": "FROM"
        },
        {
          "type": "SYMBOL",
          "name": "file_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "where_clause"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "limit_clause"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "offset_clause"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "select_list": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "column_list"
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "column_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "select_expression"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "select_expression"
              }
            ]
          }
        }
      ]
    },
    "select_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "aggregate_function"
        },
        {
          "type": "SYMBOL",
          "name": "column_name"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "column_name"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "aggregate_function": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "COUNT"
            },
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "STRING",
              "value": "*"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "COUNT"
            },
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "column_name"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "column_name": {
      "type": "SYMBOL",
      "name": "_identifier"
    },
    "file_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "where_clause": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "WHERE"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "limit_clause": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "LIMIT"
        },
        {
          "type": "SYMBOL",
          "name": "number_literal"
        }
      ]
    },
    "offset_clause": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "OFFSET"
        },
        {
          "type": "SYMBOL",
          "name": "number_literal"
        }
      ]
    },
    "expression": {
      "type": "SYMBOL",
      "name": "or_expression"
    },
    "or_expression": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "and_expression"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "and_expression"
              },
              {
                "type": "STRING",
                "value": "OR"
              },
              {
                "type": "SYMBOL",
                "name": "or_expression"
              }
            ]
          }
        ]
      }
    },
    "and_expression": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "not_expression"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "not_expression"
              },
              {
                "type": "STRING",
                "value": "AND"
              },
              {
                "type": "SYMBOL",
                "name": "and_expression"
              }
            ]
          }
        ]
      }
    },
    "not_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "NOT"
            },
            {
              "type": "SYMBOL",
              "name": "not_expression"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "primary_expression"
        }
      ]
    },
    "primary_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comparison_expression"
        },
        {
          "type": "SYMBOL",
          "name": "column_name"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "comparison_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              },
              {
                "type": "STRING",
                "value": "="
              },
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              },
              {
                "type": "STRING",
                "value": "!="
              },
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              },
              {
                "type": "STRING",
                "value": "<>"
              },
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              },
              {
                "type": "STRING",
                "value": ">"
              },
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              },
              {
                "type": "STRING",
                "value": ">="
              },
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              },
              {
                "type": "STRING",
                "value": "<"
              },
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              },
              {
                "type": "STRING",
                "value": "<="
              },
              {
                "type": "SYMBOL",
                "name": "primary_expression"
              }
            ]
          }
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "number_literal"
        },
        {
          "type": "SYMBOL",
          "name": "boolean_literal"
        },
        {
          "type": "STRING",
          "value": "NULL"
        }
      ]
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "PATTERN",
          "value": "[^']*"
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "number_literal": {
      "type": "PATTERN",
      "value": "-?\\d+(\\.\\d+)?"
    },
    "boolean_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}